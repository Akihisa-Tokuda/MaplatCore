{
  "name": "@maplat/core",
  "version": "0.7.3",
  "description": "Maplat is the cool Historical Map/Illustrated Map Viewer API.\nIt can transform each map coordinates with nonlinear but homeomorphic projection and makes possible that the maps can collaborate with GPS/accurate maps, without distorting original maps.",
  "main": "src/index.js",
  "module": "src/index.js",
  "bin": {
    "freezer": "bin/freezer.js"
  },
  "files": [
    "src/",
    "dist/",
    "parts/"
  ],
  "scripts": {
    "server": "webpack-dev-server --config ./webpack_config/webpack.config.standard.js --mode=development --devtool=inline-source-map",
    "server:packed": "webpack-dev-server --config ./webpack_config/webpack.config.packed.js  --mode=development --devtool=inline-source-map",
    "www_build": "webpack --color --progress --config ./webpack_config/webpack.config.standard.js",
    "www_build_packed": "webpack --config ./webpack_config/webpack.config.packed.js",
    "zip_build": "gulp zip",
    "build": "npm run www_build && npm run zip_build",
    "build_packed": "npm run www_build_packed",
    "test": "jest",
    "preversion": "npm run lint & npm run test",
    "version": "npm run build && npm run build_packed && git add -A",
    "preinstall": "npx npm-force-resolutions || echo 'ignore'",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,spec}/**/*.{js,ts}\" --fix",
    "lint:prettier": "prettier \"./{src,spec}/**/*.{js,ts}\" --write",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/MaplatCore.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/code4history/MaplatCore/issues"
  },
  "homepage": "https://github.com/code4history/MaplatCore/wiki",
  "dependencies": {
    "@maplat/tin": "^0.6.7",
    "@turf/turf": "^5.1.6",
    "argv": "0.0.2",
    "i18next": "^17.3.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash.template": "^4.5.0",
    "ol": "^6.1.1",
    "weiwudi": "^0.1.2",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/lodash.template": "^4.5.0",
    "@types/node": "^14.14.14",
    "@types/ol": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.1.3",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.7",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-zip": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "less": "^3.13.1",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.3",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0",
    "yargs-parser": "^18.1.3"
  },
  "resolutions": {
    "clean-css": "^4.2.3",
    "minimist": "^1.2.5",
    "yargs-parser": "^18.1.3",
    "utils-extend": "npm:utils-extend-patched@^1.0.9"
  }
}
